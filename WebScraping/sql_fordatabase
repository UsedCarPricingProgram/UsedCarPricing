CREATE TABLE `petrol` (
`index` int NOT NULL,
`title` text COLLATE utf8mb4_general_ci COMMENT '标题',
`price` float DEFAULT NULL COMMENT '价格',
`price_new` float DEFAULT NULL COMMENT '新车含税价',
`date_regi` date DEFAULT NULL COMMENT '首次上牌',
`mileage` float DEFAULT NULL COMMENT '表显里程',
`standard` varchar(10) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '排放标准',
`gearbox` tinyint DEFAULT NULL COMMENT '变速箱',
`num_trans` int DEFAULT NULL COMMENT '过户次数',
`license_location` tinyint DEFAULT NULL COMMENT '车牌地',
`color` tinyint DEFAULT NULL COMMENT '车身颜色',
`energy_type` tinyint DEFAULT NULL COMMENT '能源类型',
`displacement` float DEFAULT NULL COMMENT '排量',
`keys` int DEFAULT NULL COMMENT '钥匙数',
`brand` varchar(20) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '品牌',
`city` int DEFAULT NULL COMMENT '城市',
`price_log` float DEFAULT NULL COMMENT 'log(price)',
`rate_residual` float DEFAULT NULL COMMENT '保值率',
`num_day` int DEFAULT NULL COMMENT '使用天数',
`newness_rate` float DEFAULT NULL COMMENT '使用年限成新率',
`mile_per_day` float DEFAULT NULL COMMENT '平均每日里程数',
PRIMARY KEY (`index`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci

CREATE TABLE `mixed` (
`index` int NOT NULL,
`title` text CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci COMMENT '标题',
`price` float DEFAULT NULL COMMENT '价格',
`price_new` float DEFAULT NULL COMMENT '新车含税价',
`date_regi` date DEFAULT NULL COMMENT '首次上牌',
`mileage` float DEFAULT NULL COMMENT '表显里程',
`official_endurance` int DEFAULT NULL COMMENT '官方续航',
`standard` varchar(10) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '排放标准',
`gearbox` tinyint DEFAULT NULL COMMENT '变速箱',
`num_trans` int DEFAULT NULL COMMENT '过户次数',
`license_location` tinyint DEFAULT NULL COMMENT '车牌地',
`color` int DEFAULT NULL COMMENT '车身颜色',
`motor_power` int DEFAULT NULL COMMENT '总功率',
`battery_capacity` int DEFAULT NULL COMMENT '电池容量',
`battery_type` varchar(20) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '电池类型',
`energy_type` tinyint DEFAULT NULL COMMENT '能源类型',
`displacement` float DEFAULT NULL COMMENT '排量',
`keys` int DEFAULT NULL COMMENT '钥匙数',
`brand` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '品牌',
`city` int DEFAULT NULL COMMENT '城市',
`price_log` float DEFAULT NULL COMMENT 'log(price)',
`rate_residual` float DEFAULT NULL COMMENT '保值率',
`num_day` int DEFAULT NULL COMMENT '使用天数',
`newness_rate` float DEFAULT NULL COMMENT '使用年限成新率',
`mile_per_day` float DEFAULT NULL COMMENT '平均每日里程数',
PRIMARY KEY (`index`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci

CREATE TABLE `electric` (
`index` int NOT NULL,
`title` text COLLATE utf8mb4_general_ci COMMENT '标题',
`price` float DEFAULT NULL COMMENT '价格',
`price_new` float DEFAULT NULL COMMENT '新车含税价',
`date_regi` date DEFAULT NULL COMMENT '首次上牌',
`mileage` float DEFAULT NULL COMMENT '表显里程',
`official_endurance` int DEFAULT NULL COMMENT '官方续航',
`standard` varchar(10) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '排放标准',
`gearbox` tinyint DEFAULT NULL COMMENT '变速箱',
`num_trans` int DEFAULT NULL COMMENT '过户次数',
`license_location` tinyint DEFAULT NULL COMMENT '车牌地',
`color` tinyint DEFAULT NULL COMMENT '车身颜色',
`motor_power` int DEFAULT NULL COMMENT '总功率',
`battery_capacity` float DEFAULT NULL COMMENT '电池容量',
`battery_type` varchar(20) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '电池类型',
`energy_type` tinyint DEFAULT NULL COMMENT '能源类型',
`keys` int DEFAULT NULL COMMENT '钥匙数',
`brand` varchar(20) COLLATE utf8mb4_general_ci DEFAULT '' COMMENT '品牌',
`city` tinyint DEFAULT NULL COMMENT '城市',
`price_log` float DEFAULT NULL COMMENT 'log(price)',
`rate_residual` float DEFAULT NULL COMMENT '保值率',
`num_day` int DEFAULT NULL COMMENT '使用天数',
`newness_rate` float DEFAULT NULL COMMENT '使用年限成新率',
`mile_per_day` float DEFAULT NULL COMMENT '平均每日里程数',
PRIMARY KEY (`index`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci

